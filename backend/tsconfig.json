{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "outDir": "./dist",
    "strict": true
  },
  "include": ["**.ts", "models/oauth_google.ts"],
  "exclude": ["node_modules"]
}
